{
  "dependencies": {
    "babel": "^6.23.0",
    "express": "^4.16.2",
    "material-ui": "^1.0.0-beta.33",
    "material-ui-icons": "^1.0.0-beta.17",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-icons": "^2.2.7",
    "whatwg-fetch": "^2.0.3"
  },
  "scripts": {
